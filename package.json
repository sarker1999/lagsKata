{
  "name": "lags",
  "version": "1.0.0",
  "description": "coding exercise",
  "main": "abeas.ts",
  "dependencies": {},
  "scripts": {
    "test": "mocha -r ts-node/register ./tests/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:functional/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "functional",
      "jsx-a11y"
    ],
    "rules": {
      "@typescript-eslint/adjacent-overload-signatures": 2,
      "@typescript-eslint/await-thenable": 2,
      "@typescript-eslint/ban-ts-ignore": 2,
      "@typescript-eslint/ban-types": 2,
      "@typescript-eslint/class-name-casing": 2,
      "@typescript-eslint/consistent-type-assertions": 2,
      "@typescript-eslint/explicit-function-return-type": 2,
      "@typescript-eslint/interface-name-prefix": 2,
      "@typescript-eslint/member-delimiter-style": 2,
      "@typescript-eslint/no-array-constructor": 2,
      "@typescript-eslint/no-empty-function": 2,
      "@typescript-eslint/no-empty-interface": 2,
      "@typescript-eslint/no-explicit-any": 2,
      "@typescript-eslint/no-for-in-array": 2,
      "@typescript-eslint/no-inferrable-types": 2,
      "@typescript-eslint/no-misused-new": 2,
      "@typescript-eslint/no-misused-promises": 2,
      "@typescript-eslint/no-namespace": 2,
      "@typescript-eslint/no-non-null-assertion": 2,
      "@typescript-eslint/no-this-alias": 2,
      "@typescript-eslint/no-unnecessary-type-assertion": 2,
      "@typescript-eslint/no-unused-vars": 2,
      "@typescript-eslint/no-use-before-define": 2,
      "@typescript-eslint/no-var-requires": 2,
      "@typescript-eslint/prefer-includes": 2,
      "@typescript-eslint/prefer-namespace-keyword": 2,
      "@typescript-eslint/prefer-regexp-exec": 2,
      "@typescript-eslint/prefer-string-starts-ends-with": 2,
      "@typescript-eslint/require-await": 2,
      "@typescript-eslint/triple-slash-reference": 2,
      "@typescript-eslint/type-annotation-spacing": 2,
      "@typescript-eslint/unbound-method": 2,
      "functional/immutable-data": 2,
      "functional/no-let": 2,
      "functional/no-method-signature": 2,
      "functional/prefer-readonly-type": 2,
      "functional/no-class": 2,
      "functional/no-mixed-type": 2,
      "functional/no-this-expression": 2,
      "functional/prefer-type-literal": 2,
      "functional/no-conditional-statement": 2,
      "functional/no-expression-statement": 2,
      "functional/no-loop-statement": 2,
      "functional/no-return-void": 2,
      "functional/no-throw-statement": 2,
      "functional/no-try-statement": 2,
      "functional/functional-parameters": 2
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.8.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-functional": "^1.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "ts-node": "^8.6.2",
    "type-coverage": "^2.3.1",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5"
  },
  "author": "Shouv",
  "license": "ISC"
}
